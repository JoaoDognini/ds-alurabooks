{
  "version": "0.1.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "publicar": "npm version patch && npm publish --access=public"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "ds-alurabooks-joaodognini",
  "author": "JoaoDognini",
  "module": "dist/ds-alurabooks.esm.js",
  "size-limit": [
    {
      "path": "dist/ds-alurabooks.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ds-alurabooks.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@chromatic-com/storybook": "1.2.25",
    "@size-limit/preset-small-lib": "^11.1.1",
    "@storybook/addon-essentials": "^8.0.3",
    "@storybook/addon-interactions": "^8.0.3",
    "@storybook/addon-links": "^8.0.3",
    "@storybook/addon-onboarding": "^8.0.3",
    "@storybook/addon-webpack5-compiler-swc": "1.0.2",
    "@storybook/blocks": "^8.0.3",
    "@storybook/react": "^8.0.3",
    "@storybook/react-webpack5": "^8.0.3",
    "@storybook/test": "^8.0.3",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "husky": "^9.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^11.1.1",
    "storybook": "^8.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.0.12",
    "styled-components": "^6.1.8"
  }
}
